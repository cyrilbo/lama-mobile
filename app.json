{
  "expo": {
    "name": "lama",
    "slug": "lama",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "lama",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cyril.bonaccini.lama"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.cyril.bonaccini.lama"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./src/app/navigation"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
